{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "Binary Background.jpg",
      "use": "@vercel/static"
    },
    {
      "src": "Spyware Page.html",
      "use": "@vercel/static"
    },
    {
      "src": "Ransomware Page.html",
      "use": "@vercel/static"
    },
    {
      "src": "Trojans Page.html",
      "use": "@vercel/static"
    },
    {
      "src": "Backdoors Page.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/spyware",
      "dest": "/Spyware Page.html"
    },
    {
      "src": "/ransomware",
      "dest": "/Ransomware Page.html"
    },
    {
      "src": "/trojans",
      "dest": "/Trojans Page.html"
    },
    {
      "src": "/backdoors",
      "dest": "/Backdoors Page.html"
    }
    {
      "src": "/sitemap", 
      "dest": "/sitemap.xml" 
    }
  ]
}

